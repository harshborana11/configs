{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0f0f1090",
                "editor.background": "#0f0f1090",
                "editor.foreground": "#c3c3c3",
                "text": "#c3c3c3",
                "text.muted": "#c3c3c370",
                "text.ignored": "#c3c3c340",
                "text.placeholder": "#c3c3c350",
                "ignored": "#c3c3c330",
                "element.hover": "#c3c3c330",
                "ghost_element.hover": "#69847C30",
                "ghost_element.selected": "#69847C30",
                "ghost_element.active": "#69847C60",
                "border": "#0f0f10",
                "editor.highlighted_line.background": "#69847C10",
                "editor.active_line.background": "#69847C10",
                "panel.background": "#0f0f1090",
                "title_bar.background": "#0f0f1090",
                "title_bar.inactive_background": "#0f0f1090",
                "status_bar.background": "#0f0f1090",
                "drop_target.background": "#0f0f1090",
                "elevated_surface.background": "#0f0f10",
                "toolbar.background": "#0f0f1090",
                "tab_bar.background": "#0f0f1090",
                "tab.inactive_background": "#0f0f1090",
                "tab.active_background": "#69847C30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0f0f10",
                "scrollbar.thumb.background": "#69847C",
                "editor.gutter.background": "#0f0f1090",
                "terminal.background": "#0f0f1010",
                "terminal.foreground": "#c3c3c3",
                "terminal.dim_foreground": "#c3c3c3",
                "terminal.bright_foreground": "#c3c3c3",
                "terminal.ansi.black": "#0f0f10",
                "terminal.ansi.red": "#69847C",
                "terminal.ansi.green": "#868B7A",
                "terminal.ansi.yellow": "#759C92",
                "terminal.ansi.blue": "#7DAEA3",
                "terminal.ansi.magenta": "#A3AA92",
                "terminal.ansi.cyan": "#81B5AA",
                "terminal.ansi.white": "#c3c3c3",
                "terminal.ansi.bright_black": "#6c5959",
                "terminal.ansi.bright_red": "#69847C",
                "terminal.ansi.bright_green": "#868B7A",
                "terminal.ansi.bright_yellow": "#759C92",
                "terminal.ansi.bright_blue": "#7DAEA3",
                "terminal.ansi.bright_magenta": "#A3AA92",
                "terminal.ansi.bright_cyan": "#81B5AA",
                "terminal.ansi.bright_white": "#c3c3c3",
                "modified": "#69847C",
                "syntax": {
                    "attribute": {
                        "color": "#69847C"
                    },
                    "boolean": {
                      "color": "#868B7A"
                    },
                    "comment": {
                      "color": "#6c5959"
                    },
                    "comment.doc": {
                      "color": "#6c5959"
                    },
                    "constant": {
                      "color": "#868B7A"
                    },
                    "function": {
                      "color": "#81B5AA"
                    },
                    "keyword": {
                      "color": "#7DAEA3"
                    },
                    "number": {
                      "color": "#A3AA92"
                    },
                    "operator": {
                      "color": "#7DAEA3"
                    },
                    "string": {
                      "color": "#69847C"
                    },
                    "variable": {
                      "color": "#868B7A"
                    }
                },
                "players": [
                  {
                    "cursor": "#69847C",
                    "background": "#0f0f10",
                    "selection": "#69847C30"
                  }
                ]
            }
        }
    ]
}
